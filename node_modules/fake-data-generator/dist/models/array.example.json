{
  "config": {
    "locale": "en"
  },
  "model": {
    "company": {
      "type": "Object",
      "value": {
        "name": {
          "type": "faker",
          "value": "company.companyName"
        },
        "addresses": {
          "type": "Array",
          "options": {"size": 10},
          "value": {
            "address": {
              "type": "Object",
              "value": {
                "street": {
                  "type": "faker",
                  "value": "address.streetAddress"
                },
                "city": {
                  "type": "faker",
                  "value": "address.city"
                },
                "state": {
                  "type": "faker",
                  "value": "address.state"
                }
              }
            }
          }
        }
      }
    }
  }
}
